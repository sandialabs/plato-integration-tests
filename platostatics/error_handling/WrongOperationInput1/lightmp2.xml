<output>
  <file>output2</file>
  <format>exodus</format>
</output>
<!--
output {
  file = "output1"
}  
-->

<control>
  <time>
    <start>0.0</start>
    <end>0.01</end>
    <timestep>0.001</timestep>
  </time>
</control>
<!--
control {
  time {
    start = 0.0
    end = 0.01
    timestep = 0.001
  }
}
-->

<physics>
  <solid_statics>
    <solver>
      <output_interval>100</output_interval>
      <tolerance>1e-9</tolerance>
      <iterations>1000</iterations>
    </solver>
    <boundary_conditions>
      <displacement>
        <nodeset>1</nodeset>
        <direction>x</direction>
        <value>0.0</value>
        <scale>1.0</scale>
      </displacement>
      <displacement>
        <nodeset>1</nodeset>
        <direction>y</direction>
        <value>0.0</value>
        <scale>1.0</scale>
      </displacement>
      <displacement>
        <nodeset>1</nodeset>
        <direction>z</direction>
        <value>0.0</value>
        <scale>1.0</scale>
      </displacement>
      <traction>
        <nodeset>2</nodeset>
        <direction>y</direction>
        <value>-1e4</value>
        <scale>1.0</scale>
      </traction>
    </boundary_conditions>
  </solid_statics>
</physics>
